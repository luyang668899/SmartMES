<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://schema.qcadoo.org/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schema.qcadoo.org/view http://schema.qcadoo.org/view.xsd">

    <form name="systemIntegrationParameters">
        <fields>
            <field name="enableLogging" />
            <field name="logLevel" />
            <field name="logRetentionPeriod" />
            <field name="connectionTimeout" />
            <field name="readTimeout" />
            <field name="retryAttempts" />
            <field name="retryDelay" />
        </fields>
        <actions>
            <action name="save" type="save" />
            <action name="cancel" type="cancel" />
        </actions>
    </form>

</view>
