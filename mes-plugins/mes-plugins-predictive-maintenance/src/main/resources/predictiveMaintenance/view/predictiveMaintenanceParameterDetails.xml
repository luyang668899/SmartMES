    <page>
        <title>Predictive Maintenance Parameter Details</title>
        <form name="predictiveMaintenanceParameterForm" entity="predictiveMaintenance.predictiveMaintenanceParameter">
            <fields>
                <field name="parameter" type="string" required="true"/>
