<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://schema.qcadoo.org/model" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://schema.qcadoo.org/model http://schema.qcadoo.org/model.xsd">

    <entities>
        <entity name="qualityPrediction" package="com.qcadoo.mes.aiMl">
            <fields>
                <field name="name" type="string" required="true" />
                <field name="description" type="string" />
                <field name="product" type="manyToOne" entity="basic.product" />
                <field name="predictionModel" type="manyToOne" entity="aiMl.predictionModel" />
                <field name="predictedQuality" type="string" />
                <field name="confidence" type="decimal" />
                <field name="predictionDate" type="dateTime" />
                <field name="actualQuality" type="string" />
                <field name="accuracy" type="decimal" />
                <field name="parameters" type="json" />
                <field name="isActive" type="boolean" defaultValue="true" />
            </fields>
            <constraints>
                <constraint name="uniqueName" unique="true">
                    <field name="name" />
                </constraint>
            </constraints>
        </entity>
    </entities>

</model>
