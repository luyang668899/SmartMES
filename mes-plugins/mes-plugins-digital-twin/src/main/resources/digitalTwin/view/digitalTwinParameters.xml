<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://schema.qcadoo.org/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schema.qcadoo.org/view http://schema.qcadoo.org/view.xsd">

    <form name="digitalTwinParameters">
        <fields>
            <field name="modelFileStoragePath" />
            <field name="maxModelFileSize" />
            <field name="simulationTimeout" />
            <field name="simulationThreadPoolSize" />
            <field name="enableRealTimeUpdates" />
            <field name="updateInterval" />
            <field name="3dModelViewerEnabled" />
            <field name="3dModelViewerUrl" />
        </fields>
        <actions>
            <action name="save" type="save" />
            <action name="cancel" type="cancel" />
        </actions>
    </form>

</view>
