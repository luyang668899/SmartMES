<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://schema.qcadoo.org/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schema.qcadoo.org/view http://schema.qcadoo.org/view.xsd">

    <form name="productionPlanDetails">
        <fields>
            <field name="name" />
            <field name="description" />
            <field name="startDate" />
            <field name="endDate" />
            <field name="status" />
            <field name="createdBy" />
            <field name="createdDate" />
            <field name="lastUpdated" />
            <field name="productionOrders" />
            <field name="resources" />
            <field name="constraints" />
            <field name="optimizationParameters" />
        </fields>
        <actions>
            <action name="save" type="save" />
            <action name="cancel" type="cancel" />
            <action name="delete" type="delete" />
            <action name="optimize" type="button" />
        </actions>
    </form>

</view>
