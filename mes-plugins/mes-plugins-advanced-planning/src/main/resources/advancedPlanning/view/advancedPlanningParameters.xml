<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://schema.qcadoo.org/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schema.qcadoo.org/view http://schema.qcadoo.org/view.xsd">

    <form name="advancedPlanningParameters">
        <fields>
            <field name="geneticAlgorithmPopulationSize" />
            <field name="geneticAlgorithmMutationRate" />
            <field name="geneticAlgorithmCrossoverRate" />
            <field name="geneticAlgorithmMaxGenerations" />
            <field name="schedulingHorizon" />
            <field name="resourceUtilizationTarget" />
            <field name="enableRealTimeScheduling" />
            <field name="schedulingInterval" />
        </fields>
        <actions>
            <action name="save" type="save" />
            <action name="cancel" type="cancel" />
        </actions>
    </form>

</view>
